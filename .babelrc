{
  "env": {
    "development": {
      "presets": ["env"]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "es2015": {
              "modules": false
            },
            "targets": {
              "ie": 9,
              "uglify": true
            }
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "istanbul"
      ],
      "presets": [
        "env"
      ]
    }
  }
}
